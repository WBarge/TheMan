class ProductOptionsComponent {
    constructor(messageService, dataService) {
        this.messageService = messageService;
        this.dataService = dataService;
        this.productOptionData = new Array();
    }
    ngOnInit() {
        this.loadProductOptions();
    }
    loadProductOptions() {
        this.messageService.add({ severity: 'info', summary: 'System Message', detail: 'Loading Products' });
        this.dataService.getProductOptions().subscribe((dataSet) => {
            this.productOptionData = dataSet;
            this.messageService.clear;
        }, ex => this.handleError(ex));
    }
    clickNewProductOptionDialog() {
        if (this.newProductOption == null)
            this.newProductOption = new productOptionModel_1.ProductOptionModel();
        this.showNewProductOptionDialog = true;
    }
    createNewProductOptionDialogClick() {
        this.messageService.add({ severity: 'info', summary: 'System Message', detail: 'Saving Product' });
        const productOptionToUpload = this.newProductOption;
        this.dataService.setProductOption(productOptionToUpload).subscribe(() => this.loadProductOptions(), error => this.handleError(error));
        this.showNewProductOptionDialog = false;
    }
    clickDetailOptionDialog(productOption) {
        this.selectedProductOption = productOption;
        this.showProductOptionDetailDialog = true;
    }
    saveProductOptionDetailDialogClick() {
        this.messageService.add({ severity: 'info', summary: 'System Message', detail: 'Saving Product' });
        const productOptionToUpload = this.selectedProductOption;
        this.dataService.setProductOption(productOptionToUpload).subscribe(() => { this.messageService.clear(); }, error => this.handleError(error));
        this.showProductOptionDetailDialog = false;
    }
    handleError(errorMsg) {
        this.messageService.clear();
        this.messageService.add({ severity: 'error', summary: 'Error', detail: errorMsg });
    }
}
exports.ProductOptionsComponent = ProductOptionsComponent;
