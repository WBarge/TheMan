const rxjs_1 = __webpack_require__(/*! rxjs */ "./node_modules/rxjs/_esm2015/index.js");
class HttpErrorHandler {
    static errorHandler(error) {
        let errMsg;
        let systemErrorMessage;
        if (error instanceof http_1.HttpErrorResponse) {
            systemErrorMessage = error.error;
            errMsg = `${systemErrorMessage.exceptionType} - ${systemErrorMessage.message || ''} URL Error Happened At ${error.url}`;
        }
        else {
            errMsg = error.message ? error.message : error.toString();
        }
        console.error(errMsg);
        return rxjs_1.throwError(systemErrorMessage ? systemErrorMessage.message : errMsg);
    }
}
exports.HttpErrorHandler = HttpErrorHandler;


