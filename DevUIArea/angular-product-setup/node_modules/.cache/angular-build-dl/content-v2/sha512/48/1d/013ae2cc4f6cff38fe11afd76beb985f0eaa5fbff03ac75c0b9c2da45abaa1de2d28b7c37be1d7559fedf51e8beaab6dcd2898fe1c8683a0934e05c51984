ch(f => f());\r\n    }\r\n\r\n\r\n    writeValue(value: T) {\r\n        this.innerValue = value;\r\n    }\r\n\r\n\r\n    registerOnChange(fn: (value: T) => void) {\r\n        this.changed.push(fn);\r\n    }\r\n\r\n\r\n    registerOnTouched(fn: () => void) {\r\n        this.touched.push(fn);\r\n    }\r\n}","export class ServiceLocations {\r\n    public bannerFileUploadServiceLocation: string;\r\n    public bannerFileListServiceLocation: string;\r\n    public footerFileUploadServiceLocation: string;\r\n    public footerFileListServiceLocation: string;\r\n    public getSystemConfigurationServiceLocation: string;\r\n    public setSystemConfigurationServiceLocation: string;\r\n    public usersServiceLocation: string;\r\n    public addUserServiceLocation: string;\r\n    public rolesServiceLocation: string;\r\n    public setUserRolesLocation: string;\r\n    public getCustomersLocation: string;\r\n    public setCustomerLocation: string;\r\n    public getStatesLocation: string;\r\n    public getPhoneNumberTypesLocation: string;\r\n    public getProductsLocation: string;\r\n    public setProductLocation:string;\r\n    public getProductOptionsLocation: string;\r\n    public setProductOptionLocation: string;\r\n    public setProductOptionsForProductLocation: string;\r\n    public getOptionsForProductLocation:string;\r\n    public getAttributesLocation:string;\r\n    public setAttributeLocation: string;\r\n    public getProductAttributeLocation:string;\r\n    public setAttributesForProductLocation: string;\r\n    public getAttributesForProductOptionLocation: string;\r\n    public setAttributesForProductOptionLocation: string;\r\n    public getProductAttributeValuesLocation:string;\r\n    public setProductAttributeValuesLocation: string;\r\n    public getProductOptionAttributeValuesLocation: string;\r\n    public setProductOptionAttributeValuesLocation: string;\r\n    public createInitialProductPricingLocation: string;\r\n    public getProductsWithNoPricesLocation: string;\r\n   