                .subscribe((incomingProductOptions) => {
                this.selectedProductOptions = new Array();
                if (this.incomingPackagedProduct.options !== null) {
                    incomingProductOptions.forEach((incomingProductOption) => {
                        if (this.optionWasSelected(incomingProductOption)) {
                            this.possibleProductOptions.push(incomingProductOption);
